<%= link_to '📸＋', new_admin_post_image_path %>
<% @post_images.each do |post_image| %>
  <%= post_image.post_image %>
  犬種:<%= post_image.dog_breed %>
  説明:<%= post_image.description %>
  <%= link_to '編集', edit_admin_post_image_path(post_image) %>
  <%= link_to '削除', admin_post_image_path(post_image), method: :delete, "data-confirm" => "本当に削除しますか？" %>
<% end %>